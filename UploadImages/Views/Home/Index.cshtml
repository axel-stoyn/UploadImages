@model IEnumerable<UploadImages.Models.Picture>

@{
    ViewBag.Title = "Index";
}

<head>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="//code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/privacyLink.js"></script>
    <script src="~/Scripts/ShowImagePreview.js" type="text/javascript"></script>
    <script src="http://maps.google.com/maps/api/js?sensor=true" type="text/javascript"></script>

    <script src="http://maps.google.com/maps/api/js?sensor=true" type="text/javascript"></script>

    <script src="~/Scripts/MapLoad.js"></script>


</head>

<body>


    @*<iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d75218.84034724481!2d27.562143857836077!3d53.903512955623405!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sru!2sby!4v1489045296489" width="300" height="300" position="fixed" frameborder="0" style="border:0; right:10px; top:150px; position:absolute; float:right" allowfullscreen></iframe>*@
    <div id="mainContainer">

        <div id="leftNav">
            @foreach (var item in Model)
            {
                @Html.Raw("<img style='width:100px; height:100px; cursor:pointer; border: 2px solid #666;' class='little-preview' src=\"data:image/jpeg;base64,"
                                + Convert.ToBase64String(item.Image) + "\" />")
            }
        </div>
        <div id="downDisplay">
            <img style='width:200px; height:200px; border: 2px solid #666;' id='currentImg' class='big-preview' src="data:image/jpeg;base64,"/>
        </div>
    </div>



    <p style="position:fixed; bottom: 37%;">
        User Image Description:
        @Html.ActionLink("Edit", "Edit", null, new { id = "privacyLink" })

        <div id="privacyEdit" style="position: fixed; bottom: 10%; margin-left: 10px;"></div>
    </p>

    <p1 class="fat">
        Image Info:
    </p1>

    <div id="canvas" style="height: 300px; width:300px; position: fixed; left:75%; top: 10px;"></div>


    @foreach (var item in Model)
    {
    <p2>
        Manufacturer: @Html.ValidationMessageFor(modelitem => item.Manufactures)
    </p2>
    <p3>
        @Html.DisplayFor(modelitem => item.Model)
    </p3>
    <p4>
        @Html.DisplayFor(modelitem => item.Model)
    </p4>
    <p5>
        Compression: @Html.DisplayFor(modelitem => item.Compression)
    </p5>
    <p6>
        Exposure Time: @Html.DisplayFor(modelitem => item.Exposure)
    </p6>
    <p7>
        Exif Version: @Html.DisplayFor(modelitem => item.ExifVersion)
    </p7>
    }

    @using (Html.BeginForm("Create", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <input class="btn btn-default" type="file" name="uploadImage" style="position:fixed; bottom:2%; left: 100px" /><br>
        <input class="btn btn-default" type="submit" value="Добавить" style="position:fixed; bottom:2%; left: 2px;" />
    }
</body>